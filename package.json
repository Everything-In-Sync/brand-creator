{
  "name": "brand-creator",
  "private": true,
  "type": "module",
  "workspaces": [
    "server",
    "client"
  ],
  "scripts": {
    "dev": "npm-run-all --parallel server:dev client:dev",
    "server:dev": "npm --prefix server run dev",
    "client:dev": "npm --prefix client run dev",
    "build": "npm --prefix server run build && npm --prefix client run build",
    "start": "npm --prefix server run start"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
